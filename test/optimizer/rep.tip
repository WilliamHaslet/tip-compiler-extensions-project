perf(x,y) {
    var a,b,c,d,e,f,g,h,i,j,k,l,m,n,o,p;
    a = y+x+1;
    b = 2*y+x+1;
    c = 3*y+x+1;
    d = 4*y+x+1;
    e = 5*y+x+1;
    g = 6*y+x+1;
    h = 7*y+x+1;
    i = 8*y+x+1;
    j = 9*y+x+1;
    k = 10*y+x+1;
    l = 11*y+x+1;
    m = 12*y+x+1;
    n = 13*y+x+1;
    o = 14*y+x+1;
    p = 15*y+x+1;
    return a+b+c+d+e+g+h+i+j+k+l+m+n+o+p;
}

main(n) {
    var i, j;
    for (i: 0 .. n) {
        j = perf(20,12);
    }
    return j;
}